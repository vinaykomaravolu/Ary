func main(){
    // If statement
    if !(true && true) {
        print("false");
    }else if !false{
        print("true");
    }else{
        print("false");
    }

    // While statement
    int a = 0;
    while a < 10 {
        print(a);
        a++;
    }

    // For statement
    for int i: 1..100 {
        print(i);
    }
}